def count_negatives(arr):
    count = 0
    for num in arr:
        if num < 0:
            count += 1
    return count

# Sample Input
elements = [16, -18, 27, -16, 23, -21, 19]

# Count negatives
negative_count = count_negatives(elements)

# Output
print("Number of negative numbers in List of elements =", negative_count)
