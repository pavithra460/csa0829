# Sample array
arr = [1, 2, 3, 2, 4, 5, 1, 6, 3, 3]

# Dictionary to count frequency
freq = {}

# Count occurrences
for num in arr:
    freq[num] = freq.get(num, 0) + 1

# Print repeated elements
print("Repeated elements:")
for key, value in freq.items():
    if value > 1:
        print(f"{key} (occurs {value} times)")
